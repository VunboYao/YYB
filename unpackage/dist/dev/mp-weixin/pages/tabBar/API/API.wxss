.y-swiper {
  height: 350rpx;
}
.swiper-item {
  padding: 30rpx;
  height: 100%;
  border-radius: 10rpx;
  overflow: hidden;
}
.swiper-item image {
    width: 100%;
    height: 350rpx;
}
.y-body {
  padding: 0 40rpx;
}
.y-body ._h2 {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
    padding-left: 20rpx;
    font-size: 32rpx;
    font-weight: 600;
    line-height: 36px;
    color: #333;
}
.y-body ._h2::after {
      content: '';
      position: absolute;
      top: 20rpx;
      left: 0;
      width: 6rpx;
      height: 30rpx;
      border-radius: 2rpx;
      background: skyblue;
      display: block;
}
.y-body .hot-list {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
}
.y-body .hot-list::after {
      position: absolute;
      bottom: 0;
      left: 0;
      width: 100%;
      height: 10rpx;
      display: block;
      background: #ddd;
}
.y-body .hot-event_desc {
    padding: 10rpx 0;
}
.y-body .hot-event {
    width: calc(50% - 10rpx);
    overflow: hidden;
}
.y-body .hot-event .hot-img {
      position: relative;
      width: 100%;
      height: 230rpx;
      -webkit-box-shadow: 0 2rpx 8rpx #eee;
              box-shadow: 0 2rpx 8rpx #eee;
}
.y-body .hot-event .hot-img image {
        width: 100%;
        height: 100%;
}
.y-body .hot-event .hot-img .hot-level {
        position: absolute;
        left: 6rpx;
        bottom: 6rpx;
        color: #fff;
        font-size: 12rpx;
}
.y-body ._h3 {
    padding: 10rpx 0 6rpx 10rpx;
    font-size: 30rpx;
    font-weight: 600;
    line-height: 32rpx;
    color: #333;
}
.y-body .hot-event_time,
  .y-body .hot-event_address {
    display: block;
    font-size: 24rpx;
    line-height: 40rpx;
    color: #666;
}

