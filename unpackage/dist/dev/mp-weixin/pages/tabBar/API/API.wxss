      -ms-flex-pack: justify;
          justify-content: space-between;
  height: 88rpx;
  background: #fff;
  -webkit-box-shadow: 0 1px 2px #eee;
          box-shadow: 0 1px 2px #eee;
}
.y-headerBar .y-bar_changeCity,
  .y-headerBar .y-bar_title,
  .y-headerBar .y_bar_search {
    display: block;
    width: 30%;
}
.y-headerBar .y-bar_changeCity {
    font-size: 32rpx;
}
.y-headerBar .y-bar_changeCity .selecti-tip {
      margin-left: 2rpx;
}
.y-headerBar .y-bar_title {
    text-align: center;
}
.y-headerBar .y_bar_search {
    text-align: right;
}
